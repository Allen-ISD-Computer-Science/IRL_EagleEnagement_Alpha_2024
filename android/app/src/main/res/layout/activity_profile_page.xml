<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    tools:context=".ProfileActivity"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:id="@+id/linearProfilePage"
        style="@style/groupStylewhite_A700"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linearColumnlock"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginStart="@dimen/_27pxh"
            android:layout_marginEnd="@dimen/_27pxh"
            android:layout_weight="1.0"
            android:gravity="start"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/topBar"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginStart="0dp"
                android:layout_marginEnd="0dp"
                android:layout_weight="1.0"
                android:gravity="start"
                android:orientation="horizontal">>

                <ImageView
                    android:id="@+id/profileBackBtn"
                    android:layout_width="@dimen/_30pxh"
                    android:layout_height="@dimen/_35pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:background="@drawable/rectangle_bg_indigo_700_radius_10"
                    android:padding="5dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft_white_a700"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_arrowleft_white_a700" />

                <Space
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"></Space>

                <ImageView
                    android:id="@+id/imageLock"
                    android:layout_width="@dimen/_52pxh"
                    android:layout_height="@dimen/_56pxh"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginStart="0dp"
                    android:layout_marginEnd="0dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_lock"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_lock" />

                <Space
                    android:layout_width="@dimen/_30pxh"
                    android:layout_height="1dp"
                    android:layout_weight="1"></Space>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearEngagmentInfo"
                style="@style/groupStylecornerRadius_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20pxv"
                android:gravity="center_horizontal"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/linearRowellipsefortynine"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_15pxh"
                    android:orientation="horizontal">


                        <TextView
                            android:id="@+id/txtPoints"
                            style="@style/txtKhulaextrabold24_1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="@string/lbl_points"
                            tools:text="@string/lbl_points" />
                    <Space
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1">
                    </Space>

                        <ImageView
                            android:id="@+id/profilePointHistoryBtn"
                            android:layout_width="@dimen/_33pxh"
                            android:layout_height="@dimen/_33pxh"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:background="@drawable/rectangle_bg_indigo_901_radius_16_5"
                            android:paddingHorizontal="@dimen/_6pxh"
                            android:paddingVertical="@dimen/_6pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_vector_white_a700_33x33"
                            tools:ignore="ContentDescription" />


                        <TextView
                            android:id="@+id/txtInfoPoints"
                            style="@style/txtSolidRounded"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="5dp"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:ellipsize="end"
                            android:paddingHorizontal="10dp"
                            android:paddingTop="@dimen/_1pxv"
                            android:singleLine="true"
                            android:text="100"
                            tools:text="1" />
                </LinearLayout>

                <View
                    android:id="@+id/lineLineSeven"
                    android:layout_width="@dimen/_304pxh"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:background="@color/white_A700" />

                <LinearLayout
                    android:id="@+id/linearRowranking"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_marginEnd="@dimen/_15pxh"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txtRanking"
                        style="@style/txtKhulaextrabold24_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_ranking"
                        tools:text="@string/lbl_ranking" />
                    <Space
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1">
                    </Space>
                    <TextView
                        android:id="@+id/txtFrameSix"
                        style="@style/txtSolidRounded"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_marginEnd="5dp"
                        android:ellipsize="end"
                        android:paddingHorizontal="10dp"
                        android:paddingTop="@dimen/_1pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_50th_2"
                        tools:text="@string/lbl_50th_2" />
                </LinearLayout>

                <View
                    android:id="@+id/lineLineEight"
                    android:layout_width="@dimen/_304pxh"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:background="@color/white_A700" />

                <LinearLayout
                    android:id="@+id/linearRowrankinggrade"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txtRankingGrade"
                        style="@style/txtKhulaextrabold24_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/lbl_ranking_grade"
                        tools:text="@string/lbl_ranking_grade" />
                    <Space
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1">
                    </Space>
                    <TextView
                        android:id="@+id/txtFrameSeven"
                        style="@style/txtSolidRounded"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="5dp"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:ellipsize="end"
                        android:paddingHorizontal="10dp"
                        android:paddingTop="@dimen/_1pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_20th_1"
                        tools:text="@string/lbl_20th_1" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearStudentInfo"
                style="@style/groupStylecornerRadius_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_57pxv"
                android:gravity="center_horizontal"
                android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/linearRowstudentid"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginEnd="14dp"
                        android:layout_marginStart="17dp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/txtStudentID"
                            style="@style/txtKhulaextrabold24_1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="@string/lbl_student_id"
                            tools:text="@string/lbl_student_id" />
                        <Space
                            android:layout_width="0dp"
                            android:layout_height="1dp"
                            android:layout_weight="1">
                        </Space>
                        <TextView
                            android:id="@+id/txtInfoStudentId"
                            style="@style/txtSolidRounded"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_marginEnd="5dp"
                            android:ellipsize="end"
                            android:paddingTop="@dimen/_1pxv"
                            android:paddingHorizontal="10dp"
                            android:singleLine="true"
                            android:text="@string/lbl_123456"
                            tools:text="@string/lbl_123456" />
                    </LinearLayout>

                <View
                    android:id="@+id/lineLineSix"
                    android:layout_width="@dimen/_304pxh"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:background="@color/white_A700" />

                <LinearLayout
                    android:id="@+id/linearRowgrade"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txtGrade"
                        style="@style/txtKhulaextrabold24_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_grade2"
                        tools:text="@string/lbl_grade2" />
                    <Space
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1">
                    </Space>
<!--                    <TextView-->
<!--                        android:id="@+id/txtInfoGrade"-->
<!--                        style="@style/txtSolidRounded"-->
<!--                        android:layout_width="wrap_content"-->
<!--                        android:layout_height="wrap_content"-->
<!--                        android:layout_marginTop="@dimen/_3pxv"-->
<!--                        android:layout_marginEnd="5dp"-->
<!--                        android:ellipsize="end"-->
<!--                        android:paddingHorizontal="10dp"-->
<!--                        android:paddingEnd="@dimen/_9pxh"-->
<!--                        android:singleLine="true"-->
<!--                        android:text="@string/lbl_senior"-->
<!--                        tools:text="@string/lbl_senior" />-->

                    <Spinner
                        android:id="@+id/gradeSpinner"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:spinnerMode="dropdown"
                        android:textSize="24dp"
                        android:textColor="@color/white_A700"
                        android:fontFamily="@font/kanit"
                        style="@style/txtSolidRounded"
                        />
                </LinearLayout>

                <View
                    android:id="@+id/lineLineFour"
                    android:layout_width="@dimen/_304pxh"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:background="@color/white_A700" />                     android:layout_marginTop=&quot;@dimen/_3pxv&quot;-->&#xA;&lt;!--                        android:layout_marginEnd=&quot;5dp&quot;-->&#xA;&lt;!--                        android:ellipsi" />

                <LinearLayout
                    android:id="@+id/linearRowhouse"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txtHouse"
                        style="@style/txtKhulaextrabold24_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_house"
                        tools:text="@string/lbl_house" />
                    <Space
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1">
                    </Space>
                    <TextView
                        android:id="@+id/txtInfoHouse"
                        style="@style/txtSolidRounded"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="5dp"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:ellipsize="end"
                        android:paddingHorizontal="10dp"
                        android:paddingTop="@dimen/_1pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_400"
                        tools:text="@string/lbl_400" />
                </LinearLayout>
            </LinearLayout>

            <ImageView
                android:id="@+id/btnVectorOne"
                android:layout_width="@dimen/_50pxh"
                android:layout_height="@dimen/_50pxh"
                android:layout_gravity="end"
                android:layout_marginStart="@dimen/_255pxh"
                android:layout_marginTop="@dimen/_50pxv"
                android:layout_marginEnd="@dimen/_1pxh"
                android:background="@drawable/rectangle_bg_indigo_700_radius_10"
                android:paddingHorizontal="@dimen/_8pxh"
                android:paddingVertical="@dimen/_8pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_vector_white_a700_50x50"
                tools:ignore="ContentDescription" />
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>
