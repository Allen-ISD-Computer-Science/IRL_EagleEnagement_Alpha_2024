<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnloginbackgroun"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleindigo_901">
        <LinearLayout android:id="@+id/linearAccountCreatio"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleindigo_901">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackloginbackgroun"
                android:layout_height="@dimen/_443pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageLoginBackgroun"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_443pxh"
                    android:layout_width="match_parent"
                    android:scaleType="centerCrop"
                    imageRes="@{@drawable/img_loginbackgroun}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_loginbackgroun"/>

                <LinearLayout
                    android:id="@+id/linearColumnsignup"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|center"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:orientation="vertical">

                    <FrameLayout
                        android:id="@+id/frameStacksignup"
                        android:layout_width="@dimen/_335pxh"
                        android:layout_height="@dimen/_254pxv"
                        android:layout_marginHorizontal="@dimen/_30sp"
                        android:layout_gravity="start">

                        <TextView
                            android:id="@+id/txtSignUp"
                            style="@style/txt_1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="bottom|center"
                            android:layout_marginStart="@dimen/_28pxh"
                            android:layout_marginTop="@dimen/_16pxv"
                            android:layout_marginEnd="@dimen/_28pxh"
                            android:layout_marginBottom="@dimen/_16pxv"
                            android:ellipsize="end"
                            android:gravity="top|center"
                            android:singleLine="true"
                            android:text="@string/lbl_sign_up"
                            tools:text="@string/lbl_sign_up" />


                        <ImageView
                            android:id="@+id/img_eagleEngagementLogo"
                            imageRes="@{@drawable/img_eagleEngagementLogo}"
                            android:layout_width="315dp"
                            android:layout_height="233dp"
                            android:layout_gravity="top|center"
                            android:layout_marginHorizontal="50dp"
                            android:layout_marginStart="@dimen/_92pxh"
                            android:layout_marginEnd="@dimen/_92pxh"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_eagleEngagementLogo" />

                    </FrameLayout>

                    <TextView
                        android:id="@+id/txtStudentID"
                        style="@style/txtKhulaextrabold16_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start"
                        android:layout_marginStart="@dimen/_56pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_marginEnd="@dimen/_56pxh"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_student_id"
                        tools:text="@string/lbl_student_id" />

                    <LinearLayout
                        android:id="@+id/linearColumnzipcode"
                        style="@style/groupStyleindigo_700cornerRadius"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_56pxh"
                        android:layout_marginEnd="@dimen/_55pxh"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/txtZipcode"
                            style="@style/txtKhulaextrabold24_1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginStart="@dimen/_72pxh"
                            android:layout_marginTop="@dimen/_12pxv"
                            android:layout_marginEnd="@dimen/_71pxh"
                            android:layout_marginBottom="@dimen/_12pxv"
                            android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:singleLine="true"
                            android:text="@string/lbl_257385"
                            tools:text="@string/lbl_257385" />
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtNewPassword"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_68pxh"
                android:layout_marginStart="@dimen/_68pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_new_password"
                style="@style/txtKhulaextrabold16_2" tools:text="@string/lbl_new_password"/>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnpassword"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_68pxh"
                android:layout_marginStart="@dimen/_68pxh"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyleindigo_700cornerRadius">
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtPassword"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginEnd="@dimen/_38pxh"
                    android:layout_marginStart="@dimen/_39pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true" android:text="@string/lbl"
                    style="@style/txtKhulaextrabold24_1" tools:text="@string/lbl"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageLock"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_36pxh"
                android:layout_marginEnd="@dimen/_68pxh"
                android:layout_marginStart="@dimen/_68pxh"
                android:layout_marginTop="@dimen/_186pxv"
                android:layout_width="@dimen/_33pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_lock_white_a700"
                tools:ignore="ContentDescription" tools:src="@drawable/img_lock_white_a700"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSupport"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_68pxh"
                android:layout_marginStart="@dimen/_68pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_support"
                style="@style/txtKhulaextrabold15" tools:text="@string/lbl_support"/>
        </LinearLayout>
    </LinearLayout>
</layout>
